<%- include("partials/header") %>

<h1>Home</h1>
<p class="paragraph"><%=homeStartingContent%></p>
<div class="posts">
  <%content.forEach( (element) => {%>
    <h2 class="post-title" href="/posts/<%=element.postTitle%>" ><%=element.postTitle%></h2>
    <p class="post-content paragraph"><%=element.postContent.slice(0, 100)%>
      <a class="read-more" href="/posts/<%=element.postTitle%>"> ...Read more</a>
    </p>
    <div class="post-break"></div>
  <%})%>
</div>


<%- include("partials/footer") %>
